{
  "name": "mincu",
  "version": "2.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "preconstruct watch",
    "build": "preconstruct build",
    "prettier": "pretty-quick",
    "lint": "eslint -f unix \"package/**/*.{ts,tsx}\"",
    "docs": "typedoc --out docs/ packages/ && touch docs/.nojekyll"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@preconstruct/cli": "^2.1.0",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chalk": "^4.1.1",
    "commander": "^7.1.0",
    "eslint": "^7.15.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "fs-extra": "^9.1.0",
    "husky": "3",
    "inquirer": "^8.0.0",
    "lerna": "^4.0.0",
    "ora": "^5.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.1",
    "tsc-alias": "^1.2.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "yeoman-environment": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "globals": {
    "__DEV__": true,
    "__TEST__": true,
    "ts-jest": {
      "useBabelrc": true
    }
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "example/*"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}